@model JournalKIT.Timelesson

@{
    ViewData["Title"] = "Details";
}

<div>
   
    <dl class="row">
        <dt class = "col-sm-2">
            Номер
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Numberlessons)
        </dd>
        <dt class = "col-sm-2">
            Начало
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Startlessons)
        </dd>
        <dt class = "col-sm-2">
            Конец
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Endlessons)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Редактировать</a> |
    <a asp-action="Index">Вернуться к таблице</a>
</div>
