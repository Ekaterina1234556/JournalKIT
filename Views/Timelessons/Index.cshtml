@model IEnumerable<JournalKIT.Timelesson>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Numberlessons)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Startlessons)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Endlessons)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Numberlessons)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Startlessons)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Endlessons)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
